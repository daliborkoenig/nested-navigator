{
  "name": "nested-navigator",
  "version": "1.2.0",
  "description": "A lightweight, type-safe utility with automatic type inference to effortlessly navigate and query deeply nested objects and arrays in JavaScript and TypeScript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run build",
    "node-reset": "rm -rf node_modules && npm cache clean --force && npm install"
  },
  "keywords": [
    "typescript",
    "javascript",
    "nested",
    "objects",
    "arrays",
    "navigation",
    "query"
  ],
  "author": "Dalibor Koenig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daliborkoenig/nested-navigator"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
